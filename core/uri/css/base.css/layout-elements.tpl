.header { position: fixed; left: 0; right: 0; z-index: 1010; min-height: 59px; padding: 0 20px;
    border-bottom: 1px solid #f2f2f2; line-height: 59px; vertical-align: middle; }
    
.header .logo img { padding-top: 9px; padding-right: 5px; }    

a.logo { text-decoration: none; font-size: 20px; text-transform: uppercase; }    

a.logo:hover { color: #888; }  

.img-circle { border-radius: 50%;  }

.img-rounded { border-radius: 6px; }

.img-4-rounded { width: 40px; border-radius: 6px; }

.img-4-square { width: 40px!important; height: 40px!important; }

.img-4-circle { width: 40px!important; height: 40px!important; border-radius: 50%;  }

.img-6-circle { width: 60px!important; height: 60px!important; border-radius: 50%;  }
    
kbd { border-radius: 4px; color: #444; border-width: 1px 1px medium; 
    box-shadow: 0 2px 0 rgba(0, 0, 0, 0.2), 0 0 0 1px #ffffff inset;
    background-color: #fefefe; border-color: #ccc #ccc white; 
    border-style: solid solid none; }

blockquote { border-left: 1px solid #ccc; color: #555;}

.highlight { border-radius: 4px; padding: 9px 20px; border: 1px solid #dedede;
    background-color: #fefefe;  }
    
.highlight .tag { position: relative; top: -10px; left: -20px;
    display: inline-block; padding: 0px 10px; background: #eee;
    border: 1px solid #dedede; border-top: none; border-left: none;
    border-radius: 0px 0px 4px 0px; }

.box { border: 1px solid #f2f2f2; border-radius: 4px; min-height: 30px; }
    