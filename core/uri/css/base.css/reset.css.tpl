/* 
 * reset.css
 * html5 browser reset to reduce browser inconsitencies
 */

p, div, span, a, img, strong, em, ul, li, ol, h1, h2, h3, html, body, footer, 
header, h4, h5, h6, fieldset, form, label, legend, table, caption, tbody, tfoot, 
thead, tr, th, td, iframe, blockquote, aside, article, code, object, pre, abbr, 
dl, dt, dd, del, ins, cite, small, strike, sub, sup, tt, var, b, u, i, center, 
canvas, details, embed, figure, figcaption, hgroup, menu, nav, output, section, 
summary, time, mark, audio, video, address { margin: 0; padding: 0; border: 0;
	outline: 0; font-size: 100%; vertical-align: baseline; 
        background: transparent; }

body { line-height: 1; }

footer, header, article, nav, aside, details, figcaption, figure, hgroup,
menu, section { display: block; }

nav ul { list-style: none; }

blockquote, q { quotes: none; }

input, select { vertical-align: middle; }

del { text-decoration: line-through; }

blockquote:before, blockquote:after, q:before, q:after { content: '';  
    content: none; }

ins, mark { background-color: #dedede; color: #222; font-style: normal;
    font-weight: normal; text-decoration: none; }

abbr[title], dfn[title] { border-bottom: 1px dotted; cursor: help; }

table { border-collapse:collapse; border-spacing:0; }

caption, th, td { text-align: left; vertical-align: top; font-weight: normal; }

thead th, thead td { font-weight: bold; vertical-align: bottom; }

a img, th img, td img { vertical-align: top; }

hr { display:block; height:9px; border:0; border-bottom:1px solid #cccccc; margin: 0; margin-bottom: 10px; }

img, object, embed, video, iframe { vertical-align: middle; max-width: 100%!important; }

img, video, iframe {   height: auto!important; }

button, input, select, textarea {  margin: 0; }

textarea {   overflow: auto; }

button {   width: auto;   overflow: visible; }

input[type=button], input[type=submit], button, a { cursor: pointer; }

input[type=radio], input[type=checkbox] {   font-size: 110%; }

strong, b { font-weight: bold; }

sup { position: relative; line-height: 0; top: -0.75em; }

sub { position: relative; line-height: 0; bottom: -0.5em; }

ul, ol { margin-left: 20px; }

ul ul, ol ul, ol ol, ol ul { margin-bottom: 0px; }

ol ol li { margin-left: 4px; list-style-type: lower-alpha; }

ol ol ol li { margin-left: 10px; list-style-type: lower-roman; }

address, em { font-style: italic; }

del { text-decoration: line-through; }

pre { white-space: pre; overflow: auto; }

code { display: inline-block; line-height: 1; padding: 0 6px; }

kbd { padding: 0 6px; border-radius: 4px; white-space: nowrap; }

dl dt { font-weight: bold; }

dd {  margin-left: 1em; }



