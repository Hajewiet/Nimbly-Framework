[system-messages]
<div id="system-messages" class="callout [if system_message=(empty) echo=close]" data-close>
    <div class="float-right">
        <a>&times;</a>
    </div>
    <p>[get system_message]</p>
</div>
<div id="system-notifications" class="notification close" data-close>
    <div class="float-right">
        <a>&times;</a>
    </div>
    <p></p>
</div>
